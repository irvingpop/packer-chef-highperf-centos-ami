{
  "min_packer_version": "1.1.0",
  "variables": {},
  "builders": [
    {
      "ami_block_device_mappings": [
        {
          "device_name": "/dev/sdb",
          "virtual_name": "ephemeral0"
        },
        {
          "device_name": "/dev/sdc",
          "virtual_name": "ephemeral1"
        }
      ],
      "ami_description": "High Performance Image for AWS Native Chef Server",
      "ami_groups": [
        "all"
      ],
      "ami_name": "highperf-centos7-{{isotime \"200601021504\"}}",
      "ami_regions": [
      ],
      "communicator": "ssh",
      "ebs_optimized": true,
      "ena_support": true,
      "sriov_support": true,
      "instance_type": "t3.large",
      "region": "us-east-1",
      "subnet_id": "subnet-57bc791f",
      "source_ami": "ami-0dd362a0723a5824f",
      "ssh_pty": true,
      "ssh_username": "centos",
      "tags": {},
      "type": "amazon-ebs"
    }
  ],
  "post-processors": [],
  "provisioners": [
    {
      "destination": "/tmp/install_prereqs.sh",
      "source": "scripts/install_prereqs.sh",
      "type": "file"
    },
    {
      "inline": [
        "sudo chmod 0755 /tmp/install_prereqs.sh",
        "sudo /tmp/install_prereqs.sh"
      ],
      "inline_shebang": "/bin/bash -e",
      "type": "shell"
    }
  ]
}
